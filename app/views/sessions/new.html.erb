<div class="container">
  <%= form_for(:session, url: admin_path) do |f| %>
  <div class="row">
    <div class="input-field col s12">
      <%= f.text_field :username%>
      <%= f.label :username %>
    </div>
  </div>
  <div class="row">
    <div class="input-field col s12">
      <%= f.password_field :password%>
      <%= f.label :password %>
    </div>
  </div>
  <div class="row">
    <%= f.submit "Login", class: "waves-effect waves-light btn black" %>
  </div>
  <% end %>
</div>
